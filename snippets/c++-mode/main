#include <iostream>
using namespace std;
#include <cstdlib>
#include <string>
#include "swap.cc"
#define N 31



int main(int argc, char *argv[]) {
  
  int a[N];
  for (int i = 0; i < N; ++i) {
    a[i] = rand() % 100;
  }

  printaray(a, N);
  bubblesort(a, N);
  printaray(a, N);
  return 0;
}

